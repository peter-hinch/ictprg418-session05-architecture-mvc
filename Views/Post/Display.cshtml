<!-- _ViewStart.cshtml contains the boilerplate to be used for all views. -->

<article>
    <p>I am in display page.</p>
    <table>
        <tr><th>Title</th><td>@Html.TextBox("Title", (string)ViewBag.title, new { @id="idTitle", @class="css-class" })</td></tr>
        <tr><th>Content</th><td>@Html.TextArea("Content", (string)ViewBag.content, new { @id = "idTitle", @class = "css-class" })</td></tr>
        <tr><th>Posted</th><td>@Html.TextBox("Published On", (string)ViewBag.publishedOn, new { @id = "idTitle", @class = "css-class" })</td></tr>
        <tr><th>Author</th><td>@Html.TextBox("Published By", (string)ViewBag.publishedBy, new { @id = "idTitle", @class = "css-class" })</td></tr>
    </table>
</article>
